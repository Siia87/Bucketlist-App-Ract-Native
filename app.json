{
  "expo": {
    "name": "my-react-native-project",
    "slug": "my-react-native-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SmallBalloon.png",
    "splash": {
      "image": "./assets/Splasch.png",
      "resizeMode": "contain",
      "backgroundColor": "#C2DADD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SmallBalloon.png",
        "backgroundColor": "#C2DADD"
      }
    },
    "web": {
      "favicon": "./assets/SmallBalloon.png"
    }
  }
}